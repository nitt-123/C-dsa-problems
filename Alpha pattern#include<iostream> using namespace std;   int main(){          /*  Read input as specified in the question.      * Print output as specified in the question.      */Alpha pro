#include<iostream>
using namespace std;


int main(){
    
    /*  Read input as specified in the question.
     * Print output as specified in the question.
     */
    int n;
    cin>>n;
    for(int i = 0;i<=n;i++){
        for(int j = 0;j<=n;j++){
            if(j<i){
                char start = 'A'+i-1;
               cout<<start;
            }
        }
        cout<<endl;
    }
    return 0;
    
}

