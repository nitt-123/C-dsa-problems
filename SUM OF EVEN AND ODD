#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int rem;
    int s1 = 0;
    int s2 = 0;
    while(n!=0){
        rem = n%10;
        if(rem%2==0){
            s1+=rem;
        }else if(rem%2!=0){
            s2+=rem;
        }
        n = n/10;
    }
  cout<<s1<<" "<<s2;
  return 0;
}
